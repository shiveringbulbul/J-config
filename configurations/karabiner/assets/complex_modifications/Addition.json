{
  "title": "【 Addition 】",
  "author": "JackaL(shiveringbulbul@hotmail.com)",
  "homepage": "https://github.com/shiveringbulbul/Jackalization",
  "hostpage": "https://pqrs.org/osx/karabiner/complex_modifications/",
  "manual": "https://github.com/shiveringbulbul/Jackalization/tree/master/configurations/karabiner/assets/complex_modifications/",
  "import_url": "karabiner://karabiner/assets/complex_modifications/import?url=https://raw.githubusercontent.com/shiveringbulbul/Jackalization/master/configurations/karabiner/assets/complex_modifications/Addition.json",
  "rules": [
    {
      "description": "【 Win-Hyper 】Tab: Tab| Win + Shift",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ],
          "to": [
            {
              "key_code": "left_shift",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "【 Alt-Hyper 】`: `| Alt + Shift",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ],
          "to": [
            {
              "key_code": "left_option",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "【 Addition 】Spacebar: Spacebar|Win",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": {
            "key_code": "spacebar"
          },
          "to": {
            "key_code": "left_control"
          }
        }
      ]
    },
    {
      "description": "【 Addition 】Right-Command: Del|Alt",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": {
            "key_code": "delete_forward"
          },
          "to": {
            "key_code": "right_option"
          }
        }
      ]
    },
    {
      "description": "【 Addition 】Right-Option: F18|Ctrl",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_option",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": {
            "key_code": "f18"
          },
          "to": {
            "key_code": "right_command"
          }
        }
      ]
    },
    {
      "description": "【 Addition 】Right-Shft: ⬆︎|Shift",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": {
            "key_code": "up_arrow"
          },
          "to": {
            "key_code": "right_shift"
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow"
          },
          "to": {
            "key_code": "down_arrow"
          }
        }
      ]
    },
    {
      "description": "【 Addition 】=: =|Fn",
      "manipulators": [
        {
          "type": "basic",
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 1000,
            "basic.to_if_held_down_threshold_milliseconds": 1000
          },
          "from": {
            "key_code": "equal_sign"
          },
          "to_if_alone": {
            "key_code": "equal_sign"
          },
          "to_if_held_down": {
            "key_code": "fn"
          }
        }
      ]
    }
  ]
}
